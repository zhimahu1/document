                            {{if refundRules && refundRules.length > 0}}
                            {{each refundRules as value}}
                            {{if value.passengerType == 'ADT'}}
                            {{if value.alterFee == '免费' || value.alterFee == '不可同舱改期' }}
                            <td>{{value.alterFee}}</td>
                            {{else}}
                            <td><span class="f-red">{{value.alterFee}}</span></td>
                            {{/if}}
                            {{/if}}
                            {{/each}}
                            {{/if}}
                        </tr>
                    {{/if}}
                {{/if}}


                {{if refund}}
                    <tr>
                        <td width="74">退票规定</td>
                        <td colspan="{{refundRules.length}}" class="align-left">{{refund}}</td>
                    </tr>
                {{/if}}
                {{if rebooking}}
                    <tr>
                        <td width="74">改期规定</td>
                        <td colspan="{{refundRules.length}}" class="align-left">{{rebooking}}</td>
                    </tr>
                {{/if}}
                {{if endorse}}
                    <tr>
                        <td width="74">转签规定</td>
                        <td colspan="{{refundRules.length}}" class="align-left">{{endorse}}</td>
                    </tr>
                {{/if}}


                {{if luggallow||mealInfo}}
                    <tr>
                        <td width="74">附加说明</td>
                        <td colspan="{{refundRules.length}}" class="align-left">
                            {{luggallow}}
                            {{if sourceId != '616106' && sourceId != '616066' && sourceId != '615927'}}
                                {{mealInfo}}
                            {{/if}}
                        </td>
                    </tr>
                {{/if}}
            {{/if}}
        </table>
        <div class="info-illustrate">
            <b></b>
            {{if sourceId == '616106' || sourceId == '616066' || sourceId == '615927' || sourceId == '86385' || sourceId == '106706' || sourceId == '138669' || sourceId == '661726' || sourceId == '66301' || sourceId == '650446' || sourceId == '66361'  || sourceId == '649306' || sourceId == '66363'|| sourceId == '660846' || sourceId == '66362'}}
                <div class="illustrate-c">申请退票和改期时，航司将收回优惠金额，具体以航司实际情况为准。</div>
            {{/if}}
                <div class="illustrate-c">退改签规定以航空公司最新公布为准。</div>
        </div>
    </div>
</script>
<script id="j-tip-ticket" type="text/html">
