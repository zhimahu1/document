<script id="j-tips-plane" type="text/html">
    <div class="more-detail">
        <div class="model-detail">
            <ul class="d-left">
                <li><span>机&emsp;型：</span>{{planeStyle}}</li>
                <li><span>机&emsp;龄：</span>{{flightYear}}</li>
                <li><span>餐&emsp;食：</span>{{food}}</li>
            </ul>
            <ul class="d-right">
                <li>{{if depBridge}}<span>出发停靠廊桥率：</span>{{depBridge}}{{/if}}</li>
                <li>{{if arrBridge}}<span>到达停靠廊桥率：</span>{{arrBridge}}{{/if}}</li>
                <li>{{if cancelProb}}<span>航班取消率：</span>{{cancelProb}}{{/if}}</li>
            </ul>
        </div>
        <div class="class-detail">
            {{each flightComforts as value}}
            <div class="c-item">
                <div class="c-title">{{value.className}}</div>
                <ul class="d-left">
                    <li class="fore1"><b></b>座位间距：{{if value.seatSpace}}{{value.seatSpace}}{{else}}<span class="c-collect">收集中...</span>{{/if}}</li>
                    <li class="fore3"><b></b>座位宽度：{{if value.seatWidth}}{{value.seatWidth}}{{else}}<span class="c-collect">收集中...</span>{{/if}}</li>
                    <li class="fore5"><b></b>可倾斜度：{{if value.seatTilt}}{{value.seatTilt}}{{else}}<span class="c-collect">收集中...</span>{{/if}}</li>
                </ul>
                <ul class="d-right">
                    {{if value.media && value.media != "无"}}<li class="fore2"><b></b>{{value.media}}</li>{{/if}}
                    {{if value.power == "有"}}
                    <li class="fore4"><b></b>电源</li>
                    {{else if value.power != "无" && value.power != "可能有"}}
                    <li class="fore4"><b></b>{{value.power}}</li>
                    {{/if}}
                    {{if value.wifi == "有"}}<li class="fore6"><b></b>WIFI</li>{{/if}}
                </ul>
            </div>
            {{/each}}
        </div>
    </div>
</script>
